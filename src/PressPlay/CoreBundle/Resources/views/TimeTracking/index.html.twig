<h1>TimeTracking list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Starttime</th>
            <th>Stoptime</th>
            <th>Adjustment</th>
            <th>Sickday</th>
            <th>Holiday</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('timetracking_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.startTime }}</td>
            <td>{{ entity.stopTime }}</td>
            <td>{{ entity.adjustment }}</td>
            <td>{{ entity.sickday }}</td>
            <td>{{ entity.holiday }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('timetracking_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('timetracking_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('timetracking_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
