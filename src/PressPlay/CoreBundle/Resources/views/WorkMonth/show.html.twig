<h1>WorkMonth</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Month</th>
            <td>{{ entity.month }}</td>
        </tr>
        <tr>
            <th>Year</th>
            <td>{{ entity.year }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('admin_workmonth') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_workmonth_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('admin_workmonth_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
